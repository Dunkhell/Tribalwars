{
    "description": "Enables playing on the same account in Tribalwars in the same time for cooperants",
    "homepage_url": "https://github.com/RNabla/Tribalwars/extensions/session-sharer",
    "manifest_version": 2,
    "name": "Hermitowski.Cooperation",
    "version": "2.1.9",
    "permissions": [
        "cookies",
        "notifications",
        "*://*.plemiona.pl/*",
        "webRequest",
        "webRequestBlocking",
        "storage"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "options_ui": {
        "page": "options.html"
    }
}